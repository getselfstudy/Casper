{
  "entrypoint": "index.html",
  "fragments": [],
  "sources": [
    "tags/**/*.js",
    "index.html",
    "index.html"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "@webcomponents/webcomponentsjs/*.js"
  ],
  "builds": [
    {
      "name": "es5",
      "js": {
        "minify": true,
        "compile": "es5",
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true,
      "addPushManifest": false,
      "preset": "es5-bundled",
      "sourceMaps": true,
      "basePath": true
    },
    {
      "name": "es6",
      "browserCapabilities": [
        "es2015",
        "push"
      ],
      "js": {
        "minify": true,
        "compile": "es2015",
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": false,
      "addServiceWorker": true,
      "addPushManifest": true,
      "preset": "es6-unbundled",
      "sourceMaps": true,
      "basePath": true
    },
    {
      "name": "dev",
      "addServiceWorker": false,
      "js": {
        "minify": false,
        "compile": false
      },
      "css": {
        "minify": false
      },
      "html": {
        "minify": false
      },
      "bundle": false,
      "addPushManifest": false,
      "sourceMaps": true,
      "basePath": true
    }
  ],
  "autoBasePath": true,
  "lint": {
    "rules": [
      "polymer-3"
    ]
  },
  "npm": true,
  "componentDir": "node_modules/",
  "moduleResolution": "node"
}